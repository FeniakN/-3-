class Land:
    def handler(self):
        return "наземний"

class Water:
    def handler(self):
        return "водний"

class Boat:
    def handler(self):
        return "судно"

class Gibrid:
    def handler(self):
        return "гібрид"

land = Land()
water = Water()
boat = Boat()
gibrid = Gibrid()

# Виклик обробників для класів "Boat" і "Gibrid"
print(boat.handler())    # Виведе: судно
print(gibrid.handler())  # Виведе: гібрид
