class Error:
    def handler(self):
        return "Помилка"

class Success:
    def handler(self):
        return "Успіх"

class Start:
    def handler(self):
        return "Початок"

class B:
    def handler(self):
        return "Б"

# Створення екземплярів класів
помилка = Error()
успіх = Success()
початок = Start()
b = B()

# Виклик обробників для класів "Початок" і "В"
print(початок.handler()) # Виведе: Початок
print(b.handler()) # Виведе: Б
